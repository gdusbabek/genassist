extends layout

block scripts
    script(src='/js/async.js')
    script(src='/js/jquery-1.8.3.min.js')
    script(src='/js/jquery.cookie.js')
    script(src='/bootstrap/js/bootstrap.js')
    script(src='/js/cookies.js')

block content
    div
        a(href="/index.html") Home
    div Environment: 
        span(id="environment") #{env}
    div Context: 
        span(id="context") Dunno
        button(type="button", class="btn", onclick="setCookie('context', '!!CLEAR');") Clear it
    div Rdio Linked: 
        span(id="rdioLink") Dunno
        button(type="button", class="btn", onclick="setCookie('rdioLink', '!!CLEAR');") Clear it
    div Last.fm Linked: 
        span(id="lastLink") Dunno
        button(type="button", class="btn", onclick="setCookie('lastLink', '!!CLEAR');") Clear it
    div Last.fm User: 
        span(id="lastUser") Dunno
        button(type="button", class="btn", onclick="setCookie('lastUser', '!!CLEAR');") Clear it
    div
        a(href="/lastfm_register.html") Link with Last.fm